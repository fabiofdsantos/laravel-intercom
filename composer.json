{
    "name": "liran-co/laravel-intercom",
    "description": "Laravel wrapper for Intercom PHP.",
    "keywords": [
        "laravel",
        "intercom",
        "laravel-intercom"
    ],
    "homepage": "https://github.com/liran-co/laravel-intercom",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Liran Cohen",
            "email": "l@liran.co"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "intercom/intercom-php": "^4.4",
        "php-http/guzzle6-adapter": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LiranCo\\LaravelIntercom\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LiranCo\\LaravelIntercom\\LaravelIntercomServiceProvider"
            ],
            "aliases": {
                "Intercom": "LiranCo\\LaravelIntercom\\LaravelIntercomFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
